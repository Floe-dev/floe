{
  "name": "@floe/next",
  "version": "1.0.0-alpha.11",
  "main": "./dist/cjs/index.jsx",
  "module": "./dist/esm/index.jsx",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "prepublishOnly": "pnpm run build",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules",
    "typecheck": "npx tsc --project ./tsconfig.json --noEmit"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@swc/core": "^1.3.78",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsconfig": "workspace:*",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@floe/server": "workspace:*",
    "react-query": "^3.39.3"
  },
  "peerDependencies": {
    "next": ">=13",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
